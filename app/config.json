{
  "defaultSettings": {
    "video": {
      "autoplay": true,
      "loop": true,
      "muted": true,
      "skipInterval": 5
    },
    "UI":{
      "statusbar": {
        "enabled": true,
        "autohide": true
      },
      "playback": {
        "enabled": true,
        "autohide": true
      },
      "menubar": {
        "autohide": true
      }
    },
    "display": "fit-to-window",
    "savePath": false,
    "closeWithESC": true,
    "slideshow": {
      "interval": 7000,
      "shuffled": false
    },
    "folderendbehaviour": "openselectfolder"
  },
  "languages": {
    "en": "English",
    "de": "Deutsch"
  },
  "PRELOADRANGE": 1,
  "supportedMIMETypes": [
    "video/ogg",
    "video/webm",
    "video/mp4",
    "video/quicktime"
  ],
  "fileFilter":[
    {"name": "Mediafiles", "extensions": ["jpg", "jpeg", "png", "gif", "bmp", "webm", "mp4"]},
    {"name": "Archives", "extensions": ["zip", "cbz"]},
    {"name": "All Types", "extensions": ["*"]}
  ],
  "supportedArchivesFormats": [
    "zip", "cbz"
  ],
  "supportedMediaTypes": [
    "jpg", "jpeg", "png", "gif", "bmp", "webp", "tiff", "svg", "webm", "mp4", "mov"
  ]
}
